{
  "name": "audit-service",
  "version": "1.0.0",
  "description": "Event-driven audit logging consumer for xshopai platform",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "npx tsx watch src/server.ts",
    "debug": "npx tsx --inspect=9229 src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "docker build -t audit-service .",
    "docker:run": "docker-compose up -d"
  },
  "dependencies": {
    "@azure/monitor-opentelemetry": "^1.8.0",
    "@dapr/dapr": "^3.4.0",
    "@opentelemetry/auto-instrumentations-node": "^0.56.0",
    "@opentelemetry/exporter-zipkin": "^1.30.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.56.0",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.56.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "amqplib": "^0.10.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "tsc-alias": "^1.8.16",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "audit",
    "logging",
    "microservice",
    "postgresql",
    "express",
    "typescript"
  ],
  "author": "xshopai Team",
  "license": "MIT"
}
